FB2900-MONITORING DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    NOTIFICATION-TYPE,
    Counter32,
    Counter64,
    Gauge32,
    Integer32,
    Integer32
        FROM SNMPv2-SMI
    MODULE-COMPLIANCE,
    OBJECT-GROUP,
    NOTIFICATION-GROUP
        FROM SNMPv2-CONF
    TEXTUAL-CONVENTION,
    DisplayString,
    TimeStamp,
    TimeInterval,
    TruthValue
        FROM SNMPv2-TC
    enterprises
        FROM RFC1155-SMI;

fb2900MonitoringMIB MODULE-IDENTITY
    LAST-UPDATED "202004060000Z"
    ORGANIZATION "Andrews & Arnold Limited"
    CONTACT-INFO
        "Andrews & Arnold
        Unit 1&2, Enterprise Court
        Bracknell, Berkshire, RG12 1QS
        United Kingdom

        Tel: +44 3333 400 999
        Email: support@aa.net.uk"
    DESCRIPTION
        "This is a MIB Module for monitoring the Firebrick-specific structures
        for general system features."
    REVISION "202004060000Z"
    DESCRIPTION "Initial version of this MIB module"
    ::= { firebrick 1 }

firebrick   OBJECT IDENTIFIER ::= { enterprises 24693 }

fbVoltages OBJECT IDENTIFIER ::= { fb2900MonitoringMIB 1 }

fb2900Voltage1v1 OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 1.1V Reference in milli-Volts"
    ::= { fbVoltages 1 }

fb2900Voltage1v325 OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 1.325V Reference in milli-Volts"
    ::= { fbVoltages 2 }

fb2900Voltage1v35 OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 1.35V Reference in milli-Volts"
    ::= { fbVoltages 3 }

fb2900Voltage3v3 OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 3.3V Reference in milli-Volts"
    ::= { fbVoltages 4 }

fb2900Voltage5v0 OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 5.0V Reference in milli-Volts"
    ::= { fbVoltages 5 }

fb2900VoltagePsu OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Power Supply voltage in milli-Volts"
    ::= { fbVoltages 6 }

fb2900TrngVoltage OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "True Random Number Generator voltage in milli-Volts"
    ::= { fbVoltages 7 }

fbTemperatures OBJECT IDENTIFIER ::= { fb2900MonitoringMIB 2 }

fb2900CPUTemperature OBJECT-TYPE
    SYNTAX Integer32
    UNITS "milli-Celsius"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "CPU Temperature in milli-Celsius"
    ::= { fbTemperatures 1 }


END
