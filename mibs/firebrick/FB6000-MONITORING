FB6000-MONITORING DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    NOTIFICATION-TYPE,
    Counter32,
    Counter64,
    Gauge32,
    Integer32,
    Integer32
        FROM SNMPv2-SMI
    MODULE-COMPLIANCE,
    OBJECT-GROUP,
    NOTIFICATION-GROUP
        FROM SNMPv2-CONF
    TEXTUAL-CONVENTION,
    DisplayString,
    TimeStamp,
    TimeInterval,
    TruthValue
        FROM SNMPv2-TC
    enterprises
        FROM RFC1155-SMI;

fb6000MonitoringMIB MODULE-IDENTITY
    LAST-UPDATED "202004060000Z"
    ORGANIZATION "Andrews & Arnold Limited"
    CONTACT-INFO
        "Andrews & Arnold
        Unit 1&2, Enterprise Court
        Bracknell, Berkshire, RG12 1QS
        United Kingdom

        Tel: +44 3333 400 999
        Email: support@aa.net.uk"
    DESCRIPTION
        "This is a MIB Module for monitoring the Firebrick-specific structures
        for general system features."
    REVISION "202004060000Z"
    DESCRIPTION "Initial version of this MIB module"
    ::= { firebrick 1 }

firebrick   OBJECT IDENTIFIER ::= { enterprises 24693 }

fbVoltages OBJECT IDENTIFIER ::= { fb6000MonitoringMIB 1 }

fb6000Voltage12vAPSU OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 12V 'A' supply"
    ::= { fbVoltages 1 }

fb6000Voltage12vBPSU OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 12V 'B' supply"
    ::= { fbVoltages 2 }

fb6000VoltageCombined12V OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: Combined 12V supply"
    ::= { fbVoltages 3 }

fb6000Voltage3v3 OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 3.3V Reference in milli-Volts"
    ::= { fbVoltages 4 }

fb6000Voltage1v8 OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 1.8V Reference in milli-Volts"
    ::= { fbVoltages 5 }

fb6000Voltage1v2 OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 1.2V Reference in milli-Volts"
    ::= { fbVoltages 6 }

fb6000Voltage1v1 OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 1.1V Reference in milli-Volts"
    ::= { fbVoltages 7 }

fb6000Voltage3v3Fan OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 3.3V Fan power in milli-Volts"
    ::= { fbVoltages 8 }

fb6000Voltage1v2Fan OBJECT-TYPE
    SYNTAX Integer32
    UNITS "mV"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Voltage: 1.2V Fan power in milli-Volts"
    ::= { fbVoltages 9 }

fbTemperatures OBJECT IDENTIFIER ::= { fb6000MonitoringMIB 2 }

fb6000FanControllerTemperature OBJECT-TYPE
    SYNTAX Integer32
    UNITS "milli-Celsius"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Fan controller temperature in milli-Celsius"
    ::= { fbTemperatures 1 }

fb6000CPUTemperature OBJECT-TYPE
    SYNTAX Integer32
    UNITS "milli-Celsius"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "CPU Temperature in milli-Celsius"
    ::= { fbTemperatures 2 }

fb6000RTCTemperature OBJECT-TYPE
    SYNTAX Integer32
    UNITS "milli-Celsius"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "RTC Temperature in milli-Celsius"
    ::= { fbTemperatures 3 }

fbFanSpeeds OBJECT IDENTIFIER ::= { fb6000MonitoringMIB 3 }

fb6000Fan1Speed OBJECT-TYPE
    SYNTAX Integer32
    UNITS "rpm"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Fan 1 Speed (rpm)"
    ::= { fbFanSpeeds 1 }

fb6000Fan2Speed OBJECT-TYPE
    SYNTAX Integer32
    UNITS "rpm"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Fan 2 Speed (rpm)"
    ::= { fbFanSpeeds 2 }


END
